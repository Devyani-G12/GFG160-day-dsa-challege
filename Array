class Solution {
    void pushZerosToEnd(int[] arr) {
        // Move all zeros at end of an array 
        int non_zero =0; // to calculate the non_no. of zeros 
        for(int i =0 ;i < arr.length ;i++)
        {
            if(arr[i] != 0 )
            {   
             arr[non_zero++] = arr[i];
            }
        }
       while(non_zero <arr.length)
       {
           arr[non_zero++]=0;
       }
        
     
  }
}
